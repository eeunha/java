package com.test.question;

public class Q055 {
	public static void main(String[] args) {
		String result = "";
		for (int i = 1; i <= 100; i++) {
			int sum = 0;
			result = i + " = [";
			for (int j = 1; j <= i / 2; j++) {
				if (i % j == 0) {
					sum += j;
					result += j + ", ";

				} else {

				}
			}
		}
		System.out.println(result);
	}
}
