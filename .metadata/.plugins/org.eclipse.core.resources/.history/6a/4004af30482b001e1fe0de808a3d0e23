package com.test.question;

import java.util.Scanner;

public class Q043 {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		System.out.println("컴퓨터가 1~10 사이의 숫자를 1개 생각했습니다.\n");
		int n = 0;
		
//		int 
		
		for (int i = 0; i < 10; i++) {
			System.out.print("숫자: ");
			n = scan.nextInt();
		}
	}
}
