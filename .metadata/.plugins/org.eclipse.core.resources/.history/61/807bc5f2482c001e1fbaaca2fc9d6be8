package com.test.question;

/*

2023.07.27

Q. 아래와 같이 출력하시오.

2	7	6
9	5	1
4	3	8



*/
public class Q079 {
	public static void main(String[] args) {
		int[][] nums = new int[5][5];
		int value = 1;
		
		
		
		printArr(nums);
	}

	private static void printArr(int[][] nums) {
		// 출력 > 수정없이 그대로 사용
		for (int i = 0; i < 5; i++) {
			for (int j = 0; j < 5; j++) {
				System.out.printf("%5d", nums[i][j]);
			}
			System.out.println();
		}
	}
}
