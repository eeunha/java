package com.test.question;

import java.util.Scanner;

public class Q013 {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		System.out.print("국어: ");
		int kor = scan.nextInt();
		
		System.out.print("영어: ");
		int eng = scan.nextInt();
		
		System.out.print("수학: ");
		int mat = scan.nextInt();
	}
}
