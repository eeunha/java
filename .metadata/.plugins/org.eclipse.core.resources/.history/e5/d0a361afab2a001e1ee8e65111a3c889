		Scanner scan = new Scanner(System.in);

		System.out.print("행: ");
		int num = scan.nextInt();

		// num = 5
		// _ _ _ _ aa > 공백 4개, a 2개
		// _ _ _ abba > 공백 3개, ab ba
		// _ _ abccba > 공백 2개, abc cba
		// _ abcddcba > 공백 1개, abcd dcba
		// abcdeedcba > 공백 0개, abcde edbca

		// a = 97

		for (int i = 0; i < num; i++) {
			for (int j = 0; j < num; j++) {

			}
		}

	}
}
