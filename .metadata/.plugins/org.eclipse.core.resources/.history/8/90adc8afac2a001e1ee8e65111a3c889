package com.test.question;

public class Q053 {
	public static void main(String[] args) {
		// 2중 for문 2번 또는
		// 3중 for문 1번

		for (int i = 1; i < 10; i++) {
			for (int j = 2; j < 6; j++) {
				System.out.printf("%d X %d = %d\t\t");
			}
			System.out.println();
		}
		System.out.println();
		for (int i = 6; i < 10; i++) {

		}
	}
}
