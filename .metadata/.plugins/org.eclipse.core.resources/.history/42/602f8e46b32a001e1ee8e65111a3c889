		System.out.print("두번째 숫자: ");
		int n2 = scan.nextInt();

		String result = "";
		
		result = add(n1, n2);
		System.out.println(result);
		
		result = add(n1, n2);
		System.out.println(result);
		
		result = add(n1, n2);
		System.out.println(result);
		
		result = add(n1, n2);
		System.out.println(result);
		
		result = add(n1, n2);
